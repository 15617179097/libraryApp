.feedback_warp {
  padding: 30rpx;
}
.feedback_warp .headFeedback .feedback_content{
  width:100%;
  height:400rpx;
  background-color:white;
}
.feedback_warp .feedback_email {
  padding: 20rpx;
  height:400rpx;
  background: white;
  margin-top:20rpx;
  border-radius: 10rpx;
}
.feedback_warp .feedback_email .must{
  margin: 10rpx 0rpx;
  padding: 15rpx;
  border-radius:10rpx;
  background-color: #fff;
  height: 50rpx;
  border:2rpx solid #cfd6d7;
  font-size:30rpx ;
}
.feedback_warp .feedback_email input {
  margin: 10rpx 0rpx;
  padding: 15rpx;
  border-radius:10rpx;
  background-color: #fff;
  height: 50rpx;
  border:2rpx solid #cfd6d7;
  font-size:30rpx ;
  margin-top:14rpx;
}
.feedback_warp .feedback_content {
  padding: 20rpx;
  height:400rpx;
  background-color:white;
  border-radius: 10rpx;
}
.feedback_warp .feedback_content textarea {
  margin-top: 10rpx;
  background-color: #fff;
  border:2rpx solid #cfd6d7;
  border-radius: 10rpx;
  font-size: 26rpx;
  padding: 20rpx;
}

.feedback_warp .feedback_content .content_limit {
  background-color: #fff;
  text-align: right;
  padding-right: 10rpx;

}
.feedback_warp .feedback_but {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.feedback_warp .feedback_but button {
  width: 100%;
  background-color: #2F80ED;
  color: #fff;
  margin-top: 30rpx;
}
