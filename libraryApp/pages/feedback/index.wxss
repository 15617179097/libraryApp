.feedback_warp {
  padding: 30rpx;
}
.feedback_warp .headFeedback .feedback_content{
  width:100%;
  height:200px;
  background-color:white;
}
.feedback_warp .feedback_email {
  padding: 20rpx;
  height:200px;
  background: white;
  margin-top:10px;
  border-radius: 5px;
}
.feedback_warp .feedback_email .must{
  margin: 10rpx 0rpx;
  padding: 15rpx;
  border-radius:5px;
  background-color: #fff;
  height: 50rpx;
  border:1px solid #cfd6d7;
  font-size:15px ;
}
.feedback_warp .feedback_email input {
  margin: 10rpx 0rpx;
  padding: 15rpx;
  border-radius:5px;
  background-color: #fff;
  height: 50rpx;
  border:1px solid #cfd6d7;
  font-size:15px ;
  margin-top:7px;
}
.feedback_warp .feedback_content {
  padding: 20rpx;
  height:200px;
  background-color:white;
  border-radius: 5px;
}
.feedback_warp .feedback_content textarea {
  margin-top: 10rpx;
  background-color: #fff;
  border:1px solid #cfd6d7;
  border-radius: 5px;
  font-size: 13px;
  padding: 10px;
}

.feedback_warp .feedback_content .content_limit {
  background-color: #fff;
  text-align: right;
  padding-right: 10rpx;

}
.feedback_warp .feedback_but {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.feedback_warp .feedback_but button {
  width: 100%;
  background-color: #2F80ED;
  color: #fff;
  margin-top: 20px;
}
