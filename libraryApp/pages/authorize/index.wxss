.auth_warp {
  padding-top: 160rpx;
  text-align: center;
}
.auth_warp .auth_img{
  width:280rpx;
  height:280rpx;
  border-radius: 200rpx;
  background: white;
  margin: auto;
  border:10rpx solid #9FC8F4;
  margin-bottom:80rpx;
}
.auth_warp .auth_img image {
  /* width: 200rpx; */
  width: 260rpx;
  height: 260rpx;
}
.auth_warp .auth_title {
  font-family: 600;
}
.auth_warp .title_content {
  padding: 15rpx;
  font-size: 28rpx;
  color: #737a7c;
}
.auth_warp .auth_but {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 40rpx;
  margin-top:100rpx;
  flex-wrap: wrap;
}
.auth_warp .auth_but button {
  display: flex;
  justify-content: center;
  align-items: center;
  align-content: space-between;
  flex: auto;
  width: 80vw;
  font-size: 32rpx;
  margin-bottom: 30rpx;
}
.auth_warp .auth_but  .over{
  border: 2rpx #9b9a9a solid;
}