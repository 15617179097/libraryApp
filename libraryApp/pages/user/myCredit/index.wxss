.myInfo_warp {
  text-align: center;
  margin:auto;
  margin-top:40rpx;
  padding-top:10rpx;
  width:90%;
  height:280rpx;
  border:none;
}
.myInfo_warp .myInfo_top{
  margin-top:90rpx;
}
.myInfo_warp .myInfo_top .myInfo_title {
  font-size: 40rpx;
  font-weight: 600;
  margin-bottom:10rpx;
  color:#353636;
}
.myInfo_warp .myInfo_content {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  margin-bottom:20rpx;
  color:#353636;
}
.myInfo_warp .onclick{
  width:340rpx;
  height:50rpx;
  background: white;
  margin-top:30rpx;
  border-radius: 10rpx;
  margin-top:40rpx;
  padding-top:50rpx;
}

.myInfo_warp .onclick1,.myInfo_warp .onclick2{
  display: inline;
}
.myInfo_warp .onclick1{
  position: absolute;
  left:50rpx;
  width:155rpx;
  height:40rpx;
  border-right:1rpx solid #c6cacc;
}
.myInfo_warp .onclick1 .number,.myInfo_warp .onclick2 .number1{
  display: flex;
  flex-direction: column-reverse;
  align-items: center;
  font-size:24rpx;
  color: #6bc6f4;
}

.myInfo_warp .onclick2{
  position: absolute;
  right:70rpx;
}

