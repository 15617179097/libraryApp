
<view class="myInfo_warp" style="background:url({{imgUrl}});border-radius:20px;">
    <view class="myInfo_top">
        
        <view class="myInfo_title">
            当前信誉值
        </view>
    </view>
    <view class="myInfo_content">
    {{creditScore}}
    </view>
    <view class="progress-box" style="width:300px; margin:auto;">
        <progress percent="{{creditScore}}" stroke-width="5" activeColor="white" backgroundColor="opacity"/>
    </view>
    <view class="onclick">
        <view class="onclick1" bindtap="notes">
            <a>            
                我的记录
            </a>
            <a class="number">
                ({{subscribeSuccessCount}})
            </a>
        </view>
        <view class="onclick2" bindtap="notes">
            <a>
                我的违约
            </a>
            <a class="number1">
                ({{subscribeErrorCount}})
            </a>
        </view>
    </view>
</view>
  