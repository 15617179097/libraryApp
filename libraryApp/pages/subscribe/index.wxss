.subscribe_warp .header_title {
  margin: 20rpx;
  text-align: center;
  border-radius: 10rpx;
  padding: 10rpx;
  color: #fff;
  background-color: #8ebcf3;
}
.subscribe_warp .subscribe_today {
  padding: 50rpx;
  border: 10rpx solid #f5f1f1;
  background-color: #fff;
}
.subscribe_warp .subscribe_today .today_content .content_title {
  font-size: 40rpx;
  font-weight: 500;
}
.subscribe_warp .subscribe_today .today_content .tod_title {
  color: #8ebcf3;
}
.subscribe_warp .subscribe_today .today_content .tmw_title {
  color: #8ebcf3;
}
.subscribe_warp .today_content ._info{
  margin-top:10px;
  margin-bottom: 30px;
  color:#6c6d6d;
}
.subscribe_warp .subscribe_today .today_content .today_but,
.subscribe_warp .subscribe_today .today_content .tomorrow_but {
  color: #fff;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.subscribe_warp .subscribe_today .today_content .today_but {
  background-color: #8ebcf3;
}
.subscribe_warp .subscribe_today .today_content .tomorrow_but {
  background-color: #8ebcf3;
}
